                <li class="texto-subtitulo">
                    X
                </li>
            </ul>
        </div>


        <div class="titulo-admin" id="descuentos-titulo">
            <div class="contenedor-icono-btn">
                <i class="fa-solid fa-percent" id="icono-panel"></i></i>
            </div>
            
            <div class="texto">
                Descuentos
            </div>
        </div>

        <div class="subtitulos-contenedor">
            <ul class="subtitulos" id="descuentos-subtitulos">
                <li class="texto-subtitulo">
