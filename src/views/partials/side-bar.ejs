                <a href="/users-admin" class="btn-panel"><i class="fa-solid fa-user" id="icono-panel"></i></a>
            </div>
            
            <div class="texto">
                Usuarios
            </div>
        </div>

        <div class="subtitulos-contenedor">
            <ul class="subtitulos" id="usuarios-subtitulos">
                <li class="texto-subtitulo">
