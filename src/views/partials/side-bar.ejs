        <div class="titulo-admin" id="pedidos-titulo">
            <div class="contenedor-icono-btn">
                <i class="fa-solid fa-list-check" id="icono-panel"></i>
            </div>
            
            <div class="texto">
                Pedidos
            </div>
        </div>

        <div class="subtitulos-contenedor">
            <ul class="subtitulos" id="pedidos-subtitulos">
                <li class="texto-subtitulo">
                    X
